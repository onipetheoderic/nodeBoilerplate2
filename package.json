{
  "name": "nodeboilerplate",
  "version": "1.0.0",
  "description": "A simple boilerplate template to kickstart your project, with a sweet admin template",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app --exec babel-node",
    "start:prod": "mv *.js lib/*.js && nodemon lib/index.js --exec babel-node",
    "build": "babel lib -d dist",
    "serve": "node dist/index.js",
    "lint": "eslint ./",
    "test": "mocha --require babel-core/register ./test/**/*.js",
    "static": "webpack",
    "static:deploy": "webpack --optimize-minimize"
  },
  "nodemonConfig": {
    "ignore": [
      "*.json"
    ]
  },
  "author": "Theoderic",
  "license": "Marathon",
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "body-parser": "~1.18.2",
    "connect-flash": "^0.1.1",
    "convert-excel-to-json": "^1.6.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "excel-to-json": "^1.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "express-messages": "^1.0.1",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "hbs": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "less-middleware": "^3.1.0",
    "moment": "^2.24.0",
    "mongoose": "^5.6.2",
    "node-simple-socket": "^2.8.1",
    "pretty-error": "^2.1.1",
    "randomstring": "^1.1.5",
    "ravepay": "^1.2.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "request": "^2.88.0",
    "session-file-store": "^1.4.0",
    "sha1": "^1.1.1",
    "sha512": "0.0.1",
    "simple-excel-to-json": "^2.2.1",
    "socket.io": "^2.3.0",
    "unirest": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "autoprefixer": "^7.2.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bootstrap": "^3.4.1",
    "chai": "^4.2.0",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.43.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^1.1.6",
    "font-awesome": "^4.7.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.14.12",
    "nyc": "^14.1.1",
    "postcss-loader": "^2.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dev-utils": "^5.0.3",
    "react-dom": "^16.2.0",
    "sass-loader": "^6.0.6",
    "sinon": "^7.5.0",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
